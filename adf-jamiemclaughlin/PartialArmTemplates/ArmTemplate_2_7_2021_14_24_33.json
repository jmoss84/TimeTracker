{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-jamiemclaughlin"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Archive')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Archive Tasks",
						"description": "A trigger to archive the Tasks table.",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": "[[dbo].[sp_TaskArchive]"
						},
						"linkedServiceName": {
							"referenceName": "jamiemclaughlin",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "Archive Books",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [
							{
								"activity": "Archive Tasks",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": "[[timetracker].[sp_BookArchive]"
						},
						"linkedServiceName": {
							"referenceName": "jamiemclaughlin",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "Archive Movies",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [
							{
								"activity": "Archive Books",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": "[[timetracker].[sp_MealArchive]"
						},
						"linkedServiceName": {
							"referenceName": "jamiemclaughlin",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "Archive Meals",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [
							{
								"activity": "Archive Movies",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": "[[timetracker].[sp_MealArchive]"
						},
						"linkedServiceName": {
							"referenceName": "jamiemclaughlin",
							"type": "LinkedServiceReference"
						}
					}
				],
				"annotations": []
			},
			"dependsOn": []
		}
	]
}